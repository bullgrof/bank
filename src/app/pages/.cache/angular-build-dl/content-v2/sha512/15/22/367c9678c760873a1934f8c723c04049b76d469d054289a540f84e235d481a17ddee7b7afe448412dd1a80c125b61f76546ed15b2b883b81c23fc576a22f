let ChangePasswordPage = class ChangePasswordPage {
    constructor(activeRoute, userService, toastCtrl, aes256, platform, router) {
        this.activeRoute = activeRoute;
        this.userService = userService;
        this.toastCtrl = toastCtrl;
        this.aes256 = aes256;
        this.platform = platform;
        this.router = router;
        this.password = {
            iduser: '',
            pass: '',
            repass: ''
        };
        this.iduser = '';
        this.theResponse = {};
        this.secureKEY = '';
        this.secureIV = '';
        this.sw = 0;
        this.skey = '4FWCC-M3XVT-xxer-MKQYG-HP7YB-78784-RHPFQ-KP4R4-7841-K7X36';
        this.fchangep = _angular_forms__WEBPACK_IMPORTED_MODULE_6__["FormGroup"];
        platform.ready().then(() => {
            this.platform.pause.subscribe(() => {
                //this.logout();
            });
            this.platform.resume.subscribe(() => {
                console.log('[INFO] App resumed');
            });
        });
    }
    ngOnInit() {
        this.generateSecureKeyAndIV();
        this.iduser = this.activeRoute.snapshot.paramMap.get('iduser');
    }
    logout() {
        console.log("Logging out user");
        this.router.navigate(['/home']);
    }
    changePassword(fchangep) {
        console.log(fchangep.valid);
        if (fchangep.invalid) {
            return;
        }
        this.password.iduser = this.iduser;
        console.log(this.password);
        if (this.password.pass === this.password.repass) {
            if (this.password.pass.length < 5) {
                this.password.pass = '';
                this.password.repass = '';
                this.presentToast('La contraseña debe tener mínimo 5 caracteres');
            }
            else {
                this.userService.updatePassword(this.password).subscribe(response => {
                    this.theResponse = response;
                    if (!this.theResponse) {
                        this.password.pass = '';
                        this.password.repass = '';
                        this.presentToast('No se ha podido actualizar la contraseña');
                    }
                    else {
                        this.password.pass = '';
                        this.password.repass = '';
                        this.presentToast('Se ha cambiado la contraseña con éxito');
                        this.router.navigate(['/contacts']);
                    }
                });
            }
        }
        else {
            this.password.pass = '';
            this.password.repass = '';
            this.presentToast('Las contraseñas ingresadas no coinciden');
        }
    }
    presentToast(message) {
