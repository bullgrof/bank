.password.pass = '';\r\n            this.password.repass = '';\r\n            this.presentToast('No se ha podido actualizar la contraseÃ±a');\r\n          }else{\r\n            this.password.pass = '';\r\n            this.password.repass = '';\r\n            this.presentToast('Se ha cambiado la contraseÃ±a con Ã©xito');\r\n            this.router.navigate(['/contacts']);\r\n          }\r\n        })\r\n      }      \r\n    }else{\r\n      this.password.pass = '';\r\n      this.password.repass = '';\r\n      this.presentToast('Las contraseÃ±as ingresadas no coinciden');\r\n    }    \r\n  }\r\n\r\n  async presentToast( message: string){\r\n    const toast = await this.toastCtrl.create({\r\n      message,\r\n      duration: 3500,\r\n      position: 'middle'\r\n    });\r\n    toast.present();\r\n  }\r\n\r\n  async generateSecureKeyAndIV() {\r\n    this.secureKEY = await this.aes256.generateSecureKey(this.skey); // Returns a 32 bytes string\r\n    this.secureIV = await this.aes256.generateSecureIV(this.skey); // Returns a 16 bytes string\r\n  }\r\n\r\nverifyPasswords(rePassword){\r\n \r\n}\r\n\r\n}\r\n"],"sourceRoot":"webpack:///"}