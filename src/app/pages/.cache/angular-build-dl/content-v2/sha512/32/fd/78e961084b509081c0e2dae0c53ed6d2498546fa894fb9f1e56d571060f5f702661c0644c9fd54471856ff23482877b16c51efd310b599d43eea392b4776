 string\n    this.secureIV = await this.aes256.generateSecureIV(this.skey); // Returns a 16 bytes string\n  }\n\n  getRename() { // getContact Alias\n    let contacts = {\n      sidUser: this.mySid,\n      sidcontact: this.sidContact\n    }\n    this.contactsService.getAliasContact(contacts).subscribe(responseContacts => {\n      this.nameContact = responseContacts.alias\n    })\n  }\n\n  goToContacts() {\n    this.toResend = [];\n    this.messages.pop();\n\n\n    this.checks = 'off'\n    // this.socket.disconnect();\n    this.router.navigate(['/contacts']);\n  }\n\n  scrollToBottomInChat() {\n    setTimeout(() => {\n      if (this.content.scrollToBottom) {\n        this.content.scrollToBottom(0);\n      }\n    }, 500);\n  }\n\n  addSelectResend(msg) {\n\n    if (msg.type === '1' || msg.type === '11' || msg.type === '44') {\n      this.mess = this.encrypt(msg.body);\n    } else {\n      this.mess = msg.body;\n    }\n    if (msg.type === '1') {\n      this.typeToResend = '11';\n    } else if (msg.type === '2') {\n      this.typeToResend = '22';\n    } else {\n      this.typeToResend = msg.type;\n    }\n    if (this.toResendContacts.length === 0) {\n      this.toResendContacts.push({\n        id: msg._id,\n        body: this.mess,\n        createAt: Date.now(),\n        image: msg.image,\n        isread: '0',\n        receiverid: msg.receiverId,\n        secureIv: this.secureIV,\n        secureKey: this.secureKEY,\n        senderId: this.myId,\n        type: this.typeToResend,\n        audioFile: msg.audioFile\n      })\n      console.log('ARREGLO PARA R-ENVIAR', this.toResendContacts);\n    } else {\n      console.log('entro por el else')\n      var res = this.searchInArray(this.toResendContacts, msg);\n      console.log('RRRRTTTTT', res)\n      if (res === 0) {\n        this.toResendContacts.push({\n          id: msg._id,\n          body: this.mess,\n          createAt: Date.now(),\n          image: msg.image,\n          isread: '0',\n          receiverid: msg.receiverId,\n          secureIv: this.secureIV,\n          secureKey: this.secureKEY,\n          senderId: this.myId,\n          type: this.typeToResend,\n          audioFile: msg.audioFile\n        })\n        console.log('ARREGLO PARA R-ENVIAR POR EL ELSE', this.toResendContacts);\n      } else {\n        this.presentToast('Ya ha seleccionado este mensaje');\n        // console.log('ya se ha seleccionado el mensaje', this.toResendContacts);\n      }\n      console.log('===========CONTINUA========');\n    }\n  }\n\n  searchInArray(arr, msg) {\n    console.log('???????????????', msg)\n    console.log('???????????????', arr)\n    var found = arr.find(array => array.body === msg.body);\n    console.log('ENCONTRADO', found);\n    if (found === undefined) {\n      return 0; //not found\n      arr = '';\n    } else {\n      return 1; //founded\n      arr = '';\n    }\n\n  }\n\n  addTrunk(msg) {\n    console.log('Mensaje que ingresa desde el selct', msg);\n    if (msg.type === \"1\" || msg.type === \"11\" || msg.type === \"44\") {\n      this.mess = this.encrypt(msg.body);\n    } else {\n      this.mess = msg.body;\n    }\n    this.toResend.push({\n      body: this.mess,\n      createAt: Date.now(),\n      image: msg.image,\n      isread: '0',\n      receiverId: msg.receiverid,\n      secureIv: this.secureIV,\n      secureKey: this.secureKEY,\n      senderId: msg.senderId,\n      type: msg.type,\n      audioFile: msg.audioFile\n    })\n    // console.log('ARREGLO PARA ENVIAR', this.toResend);\n    /* this.pushService.sendPushNotificacionNew(this.notificate).subscribe(resnoti => {\n       console.log(resnoti);\n     }) */\n    console.log('ZZZZZZ', this.toResend);\n    this.mess = '';\n  }\n\n  presentSelect() {\n    this.checks = 'on'\n    this.arrowControl = 'on'\n  }\n\n  async resendMessage(resendMessage) {\n    // console.log('ARREGLO PARA ENVIAR', this.toResend);\n    if (this.toResendContacts.length > 0) {\n      // console.log(\"L1 aaaa\", this.secureIV, \"l2aaa\", this