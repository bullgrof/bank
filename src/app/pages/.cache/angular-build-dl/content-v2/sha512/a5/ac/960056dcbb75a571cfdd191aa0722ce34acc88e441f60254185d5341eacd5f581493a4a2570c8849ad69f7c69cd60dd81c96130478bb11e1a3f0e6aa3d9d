cts.page';\r\nimport { AES256 } from '@ionic-native/aes-256/ngx';\r\n\r\nimport { Howl } from 'howler';\r\nexport interface Track{\r\n  name: string;\r\n  path: string;\r\n}\r\n\r\n\r\n@Component({\r\n  selector: 'app-trunk-chat',\r\n  templateUrl: './trunk-chat.page.html',\r\n  styleUrls: ['./trunk-chat.page.scss'],\r\n})\r\nexport class TrunkChatPage implements OnInit {\r\n\r\n  idIn;\r\n  messages;\r\n  sendername='';\r\n  receivername='';\r\n  objectUser;\r\n  idsender;\r\n  idreceiver;\r\n  currentUser;\r\n  mySid;\r\n  sidC;\r\n  decodedMessage;\r\n  key1 = '123456789abcdefghijklmnopqrstuuvwxyz';\r\n\r\n  player;\r\n  isPlaying2;\r\n  activeTrack;\r\n\r\n  base = environment.url;\r\n\r\n  toResend = [];\r\n  checks = 'off';\r\n private secureKEY;\r\n private secureIV;\r\n  skey: string = environment.sekretKey;\r\n\r\n  constructor(private activatedRoute: ActivatedRoute,\r\n              private TrunkService: TrunkService,\r\n              private UserService: UsersService,\r\n              private router: Router,\r\n              private platform: Platform, \r\n              private modalCtrl: ModalController,\r\n              private toastCtrl: ToastController,\r\n              private aes256: AES256,) { \r\n                platform.ready().then(() => {\r\n                  this.platform.pause.subscribe(() => {\r\n                  //  this.logout();\r\n                  });\r\n                  this.platform.resume.subscribe(() => {\r\n                    console.log('[INFO] App resumed');\r\n                  });\r\n                })\r\n              }\r\n\r\n  ngOnInit() {\r\n    this.idIn = this.activatedRoute.snapshot.paramMap.get('id');\r\n    this.generateSecureKeyAndIV();\r\n    this.TrunkService.getTrunksById(this.idIn).subscribe(res => {\r\n      console.log('RESPUESTA SERVICIO: ',res);\r\n      this.messages = res.trunks[0].message;\r\n      console.log('===============', this.messages);\r\n      this.idsender = res.trunks[0].sender;\r\n      this.currentUser = this.idsender;\r\n      this.idreceiver = res.trunks[0].receiver;\r\n      console.log('senderId',this.idsender);\r\n      console.log('receiverId',this.idreceiver);\r\n      \r\n      this.UserService.getusernameById(this.idsender).subscribe(response => {\r\n        console.log('$$$$$$', response);\r\n        this.objectUser = response.user[0];\r\n        this.sendername = this.objectUser.username;\r\n        console.log('#$#$#$#$', this.objectUser);\r\n        this.mySid = this.objectUser.sekretid;\r\n\r\n      })\r\n\r\n      this.UserService.getusernameById(this.idreceiver).subscribe(response2 => {\r\n        console.log('$$$$$$', response2);\r\n        this.objectUser = response2.user[0];\r\n        this.receivername = this.objectUser.username;\r\n        console.log('#$#$RECEIVER#$#$', this.receivername);\r\n        this.sidC =  this.objectUser.sekretid;\r\n      })\r\n\r\n      \r\n      //this.sendername = this.objectUser\r\n    })   \r\n  }\r\n\r\n  async resendMessage(){\r\n    console.log('=AREGLO PARA REENVIAR=', this.toResend);\r\n    const modal = await this.modalCtrl.create({\r\n      component: ModalContactsPage,\r\n      componentProps: {\r\n        'objMessage': this.toResend,\r\n        'l1': this.secureIV,\r\n        'l2': this.secureKEY,\r\n        'mySid': this.mySid,\r\n        'ids': this.idsender,\r\n        'sidu' : this.sidC\r\n      }\r\n    })\r\n    modal.onDidDismiss().then(()=>{\r\n      this.canelResend();\r\n    })   \r\n\r\n    await modal.present();\r\n   \r\n  }\r\n\r\n  canelResend() {\r\n    this.toResend = [];\r\n    this.toResend = [];\r\n    // console.log('ARREGLO CANCELAR', this.toResend);\r\n    this.checks = 'off'\r\n   // this.trunkChecks = 'off';\r\n   // this.arrowControl = 'off'\r\n  }\r\n\r\n  checksOn(){\r\n    this.checks = 'on'\r\n  }\r\n\r\n  addSelectResend(msg) {\r\n    if (this.toResend.length === 0) {\r\n      this.toResend.push({\r\n        id: msg._id,\r\n        body: msg.body,\r\n        createAt: Date.now(),\r\n        image: msg.image,\r\n        isread: '0',\r\n        receiverid: msg.receiverId,\r\n        secureIv: this.secureIV,\r\n        secureKey: this.secureKEY,\r\n        senderId: this.idsender,\r\n        type: msg.type,\r\n        audioFile: msg.audioFile\r\n      })\r\n      console.log('ARREGLO PARA R-ENVIAR', this.toResend);\r\n    } else {\r\n      console.log('entro por el else')\r\n      var res = this.searchInArray(this.toResend, msg);\r\n      console.log('RRRRTTTTT', res)\r\n      if (res === 0) {\r\n        this.toResend.push({\r\n          id: msg._id,\r\n          body: msg.body,\r\n          createAt: Date.now(),\r\n          image: msg.image,\r\n          isread: '0',\r\n          receiverid: msg.receiverId,\r\n          secureIv: this.secureIV,\r\n          secureKey: this.secureKEY,\r\n          senderId: this.idsender,\r\n          type: msg.type,\r\n          audioFile: msg.audioFile\r\n        })\r\n        console.log('ARREGLO PARA R-ENVIAR POR EL ELSE', this.toResend);\r\n      } else {\r\n        this.presentToast('Ya ha seleccionado este mensaje');\r\n       // console.log('ya se ha seleccionado el mensaje', this.toResendContacts);\r\n      }\r\n      console.log('===========CONTINUA========');\r\n    }\r\n  }\r\n\r\n  searchInArray(arr, msg) {\r\n    console.log('???????????????', msg)\r\n    console.log('???????????????', arr)\r\n    var found = arr.find(array => array.body === msg.body);\r\n    console.log('ENCONTRADO', found);\r\n    if (found === undefined) {\r\n      return 0; //not found\r\n      arr = '';\r\n    } else {\r\n      return 1; //founded\r\n      arr = '';\r\n    }\r\n\r\n  }\r\n\r\n  async presentToast(message: string) {\r\n    const toast = await this.toastCtrl.create({\r\n      message,\r\n      duration: 3000,\r\n      position: 'middle'\r\n    });\r\n    toast.present();\r\n  }\r\n\r\n\r\n  logout() {\r\n    console.log(\"Logging out user\");\r\n    this.router.navigate(['/home']);\r\n  }\