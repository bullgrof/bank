k\\\" slot=\\\"start\\\" ></ion-icon>\\n      <ion-label>Password: {{ps}} </ion-label>\\n    </ion-item>\\n  </ion-card>\\n\\n</ion-content>\\n\"","import { NgModule } from '@angular/core';\nimport { CommonModule } from '@angular/common';\nimport { FormsModule } from '@angular/forms';\nimport { Routes, RouterModule } from '@angular/router';\n\nimport { IonicModule } from '@ionic/angular';\n\nimport { CallinfoPage } from './callinfo.page';\n\nconst routes: Routes = [\n  {\n    path: '',\n    component: CallinfoPage\n  }\n];\n\n@NgModule({\n  imports: [\n    CommonModule,\n    FormsModule,\n    IonicModule,\n    RouterModule.forChild(routes)\n  ],\n  declarations: [CallinfoPage]\n})\nexport class CallinfoPageModule {}\n","export default \"\\n/*# sourceMappingURL=data:application/json;base64,eyJ2ZXJzaW9uIjozLCJzb3VyY2VzIjpbXSwibmFtZXMiOltdLCJtYXBwaW5ncyI6IiIsImZpbGUiOiJzcmMvYXBwL3BhZ2VzL2NhbGxpbmZvL2NhbGxpbmZvLnBhZ2Uuc2NzcyJ9 */\"","import { Component, OnInit } from '@angular/core';\nimport { Storage } from '@ionic/storage';\nimport { Clipboard } from '@ionic-native/clipboard/ngx';\nimport { ToastController } from '@ionic/angular';\n\nimport { UsersService } from 'src/app/services/users.service';\n\n@Component({\n  selector: 'app-callinfo',\n  templateUrl: './callinfo.page.html',\n  styleUrls: ['./callinfo.page.scss'],\n})\nexport class CallinfoPage implements OnInit {\n\n  se;\n  ps;\n  tex;\n  msj='';\n\n  constructor(\n    private storage: Storage,\n    private userService: UsersService,\n    private clipboard: Clipboard,\n    private toastCtrl: ToastController,\n  ) { }\n\n  ngOnInit() {\n    this.storage.get('username').then((result) => {\n      this.userService.getSekretId(result).subscribe(response => {\n        if (response.user[0].se === '' || response.user[0].se === undefined) {\n          this.se = 'Por asignar';\n        }else {\n          this.se = response.user[0].se;\n        }\n        if (response.user[0].ps === '' || response.user[0].ps === undefined) {\n          this.ps = 'Por asignar';          \n        }else{\n          this.ps = response.user[0].ps;\n        }\n        if (response.user[0].tex === '' || response.user[0].tex === undefined) {\n          this.tex = 'Por asignar';\n        }else{\n          this.tex = response.user[0].tex;\n        }       \n      });\n    });\n  }\n\n  copyClipboard(param, num) {\n    console.log(num)\n    if (num === '1') {\n      this.msj = 'Se ha copiado el servidor'\n    }else if (num === '2') {\n      this.msj = 'Se ha copiado el Usuario'\n    }else if (num ==='3') {\n      this.msj = 'Se ha copiado el password'\n    }\n    this.clipboard.copy(param);\n    this.presentToast(this.msj);\n  }\n\n  async presentToast(message: string) {\n    const toast = await this.toastCtrl.create({\n      message,\n      duration: 2500,\n      position: 'middle'\n    });\n    toast.present();\n  }\n\n}\n"],"sourceRoot":"webpack:///"}