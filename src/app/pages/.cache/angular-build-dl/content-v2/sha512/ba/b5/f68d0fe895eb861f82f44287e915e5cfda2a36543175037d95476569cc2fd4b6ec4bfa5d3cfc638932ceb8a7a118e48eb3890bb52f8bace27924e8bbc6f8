            // console.log('image upload******', this.dataResFiles.name);
            if (this.dataResFiles === '') {
                setTimeout(() => {
                    //  console.log('paso al if')
                    rs = this.dataResFiles.name;
                    this.sendMessageImage(this.dataResFiles.name);
                }, 1000);
            }
            else {
                // console.log('paso al ELSE')
