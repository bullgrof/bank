                this.activeTrack = track;
            },
            onend: () => {
                console.log('OnEnd');
            }
        });
        this.player.play();
    }
    togglePlayer(pause) {
        this.isPlaying = !pause;
        if (pause) {
            this.player.pause();
        }
        else {
            this.player.play();
        }
    }
