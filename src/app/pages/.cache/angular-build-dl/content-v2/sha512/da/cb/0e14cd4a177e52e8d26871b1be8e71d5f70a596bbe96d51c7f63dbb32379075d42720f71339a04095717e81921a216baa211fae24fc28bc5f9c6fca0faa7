    verifyt(token) {
        console.log('Ingresa por revision: ', token);
        let characters = token.length;
        if (characters === 20) {
            this.tokenService.getVerifyToken(token).subscribe(resp => {
                console.log('resp token', resp);
                if (resp.tokens[0].token != null && resp.tokens[0].state === '1') {
                    this.idplan = resp.tokens[0].idplan;
                    this.presentToast('El token se encuentra disponible');
                    this.getDays(this.idplan);
                }
                else {
                    this.userRegisterValid.idtoken = 1;
                    this.presentToast('El token No esta disponible');
                    this.idplan = 1010;
                }
            });
        }
        else {
            this.presentToast('El token No esta registrado');
            this.userRegisterValid.idtoken = 1;
        }
        return this.idplan;
    }
    getDays(idplan) {
        let idplanToService = idplan;
        //console.log('IDPLAN TO SEARCH', idplanToService)
        this.tokenService.getRemainingDays(idplan).subscribe(resp => {
            console.log('PLAN AL QUE PERTENECE', resp);
            this.typePlan = resp.plans[0].type;
            console.log('TAMALO DE LOSP PLANES ', resp.plans.length);
            if (resp.plans.length === 0) {
                this.presentToast('El token No se encuentra registrado.');
            }
            console.log('DIAS DEL PLAN', resp.plans[0].timetoexpire);
            this.timeToken = resp.plans[0].timetoexpire;
            var hoy = new Date();
            var devolucion = new Date();
            devolucion.setDate(hoy.getDate() + this.timeToken);
            console.log('FECHA ACTUAL', Date());
            this.testing = devolucion.toLocaleDateString();
        });
        return this.testing;
    }
