    scan() {
        this.qrScanner.prepare().then((status) => {
            if (status.authorized) {
                this.qrScanner.show();
                document.getElementsByTagName("body")[0].style.opacity = "0";
                this.scaned = this.qrScanner.scan().subscribe((textFound) => {
                    document.getElementsByTagName("body")[0].style.opacity = "1";
                    this.users.idtoken = textFound;
                    this.txt = textFound;
                    this.verifyt(this.txt);
                    console.log('TEXTO ESCANEADO', textFound);
                    this.qrScanner.hide();
                    this.scaned.unsubscribe();
                    this.qrScanner.destroy();
                    this.users.username.focus();
                    this.scaned.destroy();
                }, (err) => {
                    console.log(err);
                });
            }
            else if (status.denied) {
            }
            else {
            }
        });
    }
    passwordsmatch(pass1, pass2) {
        console.log('passoword 1', pass1);
        console.log('passoword 2', pass2);
        if (pass1 === pass2) {
            this.passwordsnotmatch = 'Correcto';
            this.classpassinput = 'match';
            this.userRegisterValid.password = 0;
        }
        else {
            this.passwordsnotmatch = 'Las contraseñas no coinciden';
            this.classpassinput = 'dontmatch';
            this.userRegisterValid.password = 1;
        }
    }
    verifyu(user) {
        this.userService.getVerifyUser(user).subscribe(response => {
            this.verifyUser = response.user[0];
            console.log('+++++++++++', this.verifyUser);
            if (!this.verifyUser) {
                this.presentToast('Usuario disponible');
            }
            else {
                this.presentToast('Usuario no disponible');
            }
        });
    }
    inputValidator(event) {
        console.log('paso al metod de validacion');
        console.log(event.target.value);
        const pattern = /^[a-zA-Z0-9]*$/;
        //let inputChar = String.fromCharCode(event.charCode)
        if (!pattern.test(event.target.value)) {
            event.target.value = event.target.value.replace(/[^a-zA-Z0-9]/g, "");
            // invalid character, prevent input
        }
    }
