                        text: 'Aceptar',
                        handler: () => {
                            console.log('Confirm Okay');
                            console.log('ENTRA EN EL CHECJ', check);
                            if (check === true) {
                                this.users.check = true;
                                this.userRegisterValid.check = 0;
                            }
                        }
                    }
                ]
            });
            yield alert.present();
        });
    }
