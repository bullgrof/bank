              secureIv: this.secureIV,
              secureKey: this.secureKEY,
              senderId: this.myId,
              type: msg.type,
              audioFile: msg.audioFile
            });
            console.log('ARREGLO PARA R-ENVIAR', this.toResendContacts);
          } else {
            console.log('entro por el else');
            var res = this.searchInArray(this.toResendContacts, msg);
            console.log('RRRRTTTTT', res);

            if (res === 0) {
              this.toResendContacts.push({
                id: msg._id,
                body: msg.body,
                createAt: Date.now(),
                image: msg.image,
                isread: '0',
                receiverid: msg.receiverId,
                secureIv: this.secureIV,
                secureKey: this.secureKEY,
                senderId: this.myId,
                type: msg.type,
                audioFile: msg.audioFile
              });
              console.log('ARREGLO PARA R-ENVIAR POR EL ELSE', this.toResendContacts);
            } else {
              this.presentToast('Ya ha seleccionado este mensaje'); // console.log('ya se ha seleccionado el mensaje', this.toResendContacts);
            }

            console.log('===========CONTINUA========');
          }
        }
      }, {
        key: "searchInArray",
        value: function searchInArray(arr, msg) {
          console.log('???????????????', msg);
          console.log('???????????????', arr);
          var found = arr.find(function (array) {
            return array.body === msg.body;
          });
          console.log('ENCONTRADO', found);

          if (found === undefined) {
            return 0; //not found

            arr = '';
          } else {
            return 1; //founded

            arr = '';
          }
        }
